{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "display": {
    "separator": " ",
  },
  "logo": {
    "type": "kitty",
    "source": "$XDG_CONFIG_HOME/fastfetch/shogun3.webp",
    "padding": {
      "top": 1
    },
    "color": {
     "1": "31",
     "2": "31"
    }
  },
  "modules": [
    {
      "type": "title",
      "format": "{##CBA6F7}{1}{#0}@{##CBA6F7}{2}{#0}"
    },
    {
      "type": "custom",
      "format": "┌────────────────◉────────────────┐"
    },
    "break",
    {
      "type": "os",
      "key": " {#33} "
    },
    {
      "type": "wm",
      "key": " {#33} ",
      "format": "{2} -- {3}"
    },
    {
      "type": "terminal",
      "key": " {#33} ",
      "format": "{5} {6}"
    },
    {
      "type": "wallpaper"
    },
    {
      "type": "cpu",
      "key": " {#33} ",
      "format": "{1}"
    },
    {
      "type": "gpu",
      "key": " {#33}󰢮 ",
      "format": "{#1}[{##CBA6F7}{6}{#0}{#1}]{#0} {2}"
    },
    {
      "type": "memory",
      "key": " {#33} "
    },
    {
      "type": "disk",
      "key": " {#33} ",
      "format": "{#1}[{#35}{10}{#0}{#1}]{#0} {1} / {2} ({#32}{3}{#0})"
    },
    {
      "type": "battery",
      "key": " {#33} ",
      "format": "{#32}{4}{#0} [{5}]"
    },
    {
      "type": "uptime",
      "key": " {#33}󰥔 "
    },
    "break",
    {
      "type": "custom",
      "format": "└────────────────◉────────────────┘"
    }
  ]
}
